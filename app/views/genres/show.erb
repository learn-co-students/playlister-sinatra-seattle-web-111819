<h2>Genre:</h2>
<p>Name: <%= @genre.name %></p>
<br>
<h2>Artists:</h2>
<% @artists.each do |artist| %>
    <%= "<a href='/artists/#{artist.slug}'>"%><%= artist.name %></a><br>
<% end %>
<br>
<h2>Songs:</h2>
<% @songs.each do |song| %>
    <%= "<a href='/songs/#{song.slug}'>"%><%= song.name %></a><br>
<% end %>